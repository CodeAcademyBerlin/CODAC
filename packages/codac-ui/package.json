{
  "name": "codac-ui",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts",
      "./src/input.css"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/*",
        "./src/*/index"
      ]
    }
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.9",
    "@storybook/addon-interactions": "7.0.9",
    "@storybook/addon-links": "7.0.9",
    "@storybook/addon-styling": "^1.0.3",
    "@storybook/blocks": "7.0.9",
    "@storybook/manager-api": "^7.0.7",
    "@storybook/nextjs": "7.0.9",
    "@storybook/react": "7.0.9",
    "@storybook/testing-library": "0.0.14-next.2",
    "@storybook/theming": "^7.0.9",
    "@tailwindcss/forms": "^0.5.0",
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.1.0",
    "next": "12.1.5",
    "postcss": "^8.4.23",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "storybook": "7.0.9",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.0.24",
    "tsconfig": "workspace:*"
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsc --noEmit",
    "build2": "pnpm build:css && pnpm run build:js",
    "dev2": "concurrently --names css,js --kill-others \"pnpm dev:css\" \"pnpm dev:js\"",
    "dev:js": "tsup src/index.ts --format esm,cjs --external react,next --watch --onSuccess \"tsc --emitDeclarationOnly --declaration\"",
    "dev:css": "pnpm tailwindcss -i ./src/input.css -o ./dist/styles/main.css --watch",
    "build2:css": "pnpm tailwindcss -i ./src/input.css -o ./dist/styles/main.css",
    "build2:js": "tsup src/index.ts --format esm,cjs --external react,next --dts",
    "storybook": "storybook dev -p 6006",
    "storybook-build": "storybook build -o ./storybook-static"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "1.0.6",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/react-table": "^8.0.0-alpha.26",
    "clsx": "1.2.1",
    "date-fns": "2.29.3",
    "dinero.js": "2.0.0-alpha.8",
    "ms": "3.0.0-canary.1",
    "next": "13.3.1-canary.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-particles": "^2.9.3",
    "server-only": "0.0.1",
    "styled-components": "6.0.0-beta.2",
    "tsparticles": "^2.9.3",
    "use-count-up": "3.0.1"
  },
  "peerDependencies": {
    "next": "12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  }
}
