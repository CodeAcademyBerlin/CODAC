{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "env": ["NEXT_PUBLIC_CODAC_ADMINISTRATION_URL"],
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "storybook": {},
    "storybook-build": {
      "outputs": ["storybook-static/**"]
    },
    "lint": {},
    "check-types": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
